<div class="container max-w-sm mx-auto">
<div class="flex center-content justify-between py-4">

<div class="button-log">
<% if user_signed_in?  %>

  <% links.each do |link| %>
    <%= link_builder(link) %>
  <% end %>

  <%= render "/meal_plans/new_meal", meals: @seven_random_dinners, user: current_user %>
<% else %>
  <%= link_to 'Sign In', new_user_session_path, class: "justify-center text-white bg-green-500 hover:bg-green-700 font-bold py-2 px-4 rounded-full"%>
<% end %>
</div>
</div>
